{
  "name": "davidhirtz/yii2-shop",
  "type": "yii2-extension",
  "description": "Yii2 Shop Module",
  "homepage": "http://www.davidhirtz.com/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "davidhirtz/yii2-media": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "davidhirtz\\yii2\\shop\\": ""
    }
  },
  "extra": {
    "bootstrap": "davidhirtz\\yii2\\shop\\composer\\Bootstrap"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:davidhirtz/yii2-media.git"
    }
  ]
}