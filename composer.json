{
  "name": "davidhirtz/yii2-shopify",
  "type": "yii2-extension",
  "description": "Yii2 Shopify Module",
  "homepage": "https://www.davidhirtz.com/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "shopify/shopify-api": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "davidhirtz\\yii2\\shopify\\": ""
    }
  },
  "extra": {
    "bootstrap": "davidhirtz\\yii2\\shopify\\composer\\Bootstrap"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:davidhirtz/yii2-cms.git"
    }
  ]
}